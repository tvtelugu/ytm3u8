{
  "version": 2,
  "builds": [
    {
      "src": "index.php",
      "use": "@vercel/php"
    },
    {
      "src": "api.php",
      "use": "@vercel/php"
    },
    {
      "src": "stream.php",
      "use": "@vercel/php"
    },
    {
      "src": "_configs.php",
      "use": "@vercel/php"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "index.php"
    }
  ]
}
